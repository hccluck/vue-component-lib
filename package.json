{
  "name": "vue-component-lib",
  "version": "1.0.0",
  "private": true,
  "description": "A Component Library for Vue.js.",
  "scripts": {
    "dev": "cross-env VUE_APP_ENV=dev vue-cli-service serve --open",
    "build": "cross-env VUE_APP_ENV=pro vue-cli-service build",
    "build:lib": "cross-env VUE_APP_ENV=lib vue-cli-service build",
    "build:report": "cross-env VUE_APP_ENV=pro REPORT=TRUE vue-cli-service build",
    "lint": "vue-cli-service lint --fix"
  },
  "main": "./lib/index/index.js",
  "files": [
    "lib/*",
    "packages/*",
    "*.json",
    "*.js",
    "*.md"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "highlight.js": "^10.7.3",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/component-compiler-utils": "^3.2.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^8.0.3",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "prettier": "^2.2.1",
    "style-resources-loader": "^1.4.1",
    "transliteration": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.1.0"
  },
  "keywords": [
    "vue",
    "components",
    "vue-component-lib"
  ],
  "license": "ISC"
}
